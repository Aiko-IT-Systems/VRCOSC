<Window x:Class="VRCOSC.App.Pages.Modules.ModuleSettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:modules="clr-namespace:VRCOSC.App.Modules"
        mc:Ignorable="d"
        Title="ModuleSettingsWindow" Height="450" Width="800">
    <Window.DataContext>
        <modules:Module />
    </Window.DataContext>
    <Grid>
        <ListBox ItemsSource="{Binding Settings}" HorizontalAlignment="Stretch" HorizontalContentAlignment="Stretch">
            <ListBox.ItemTemplate>
                <DataTemplate>
                    <Frame Content="{Binding PageInstance, Mode=OneTime}"/>
                </DataTemplate>
            </ListBox.ItemTemplate>
        </ListBox>
    </Grid>
</Window>
