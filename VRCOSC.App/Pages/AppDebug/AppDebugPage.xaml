<Page x:Class="VRCOSC.App.Pages.AppDebug.AppDebugPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:ui="clr-namespace:VRCOSC.App.UI"
      mc:Ignorable="d">
    <Grid>
        <Border Background="{StaticResource CBackground2}" Padding="10">
            <ui:SpacedStackPanel Spacing="5" Orientation="Horizontal">
                <TextBlock Text="{Binding Port9000BoundProcess.Value}" FontSize="20" Foreground="{StaticResource CForeground1}" />
            </ui:SpacedStackPanel>
        </Border>
    </Grid>
</Page>
